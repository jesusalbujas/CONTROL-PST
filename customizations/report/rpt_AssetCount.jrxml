	<parameter name="M_Product_ID" class="java.math.BigDecimal"/>
	<parameter name="RECORD_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
    m.M_Product_ID,
    m.Name AS ProductName,
    COUNT(fa.A_Asset_ID) AS AssetCount,
    inf.Logo_ERP AS Logo_ERP
FROM
    M_Product m
 LEFT JOIN
    A_Asset fa ON fa.M_Product_ID = m.M_Product_ID
 LEFT JOIN
    AD_Client ac ON fa.AD_Client_ID = ac.AD_Client_ID
 LEFT JOIN
    AD_ClientInfo inf ON fa.AD_Client_ID = inf.AD_Client_ID
WHERE (m.M_Product_ID = $P{RECORD_ID} OR $P{RECORD_ID} IS NULL)
	AND (m.M_Product_ID = $P{M_Product_ID} OR $P{M_Product_ID} IS NULL)
    AND (fa.AD_Org_ID = 50006 OR fa.AD_Org_ID IS NULL)
	AND (m.AD_Org_ID = 50006 OR m.AD_Org_ID IS NULL)
    AND (fa.IsDisposed = 'N' OR fa.IsDisposed IS NULL)
GROUP BY
    m.M_Product_ID, m.Name, inf.Logo_ERP
ORDER BY
    m.Name ASC]]>
	</queryString>
